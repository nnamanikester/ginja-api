{
    "name": "ginjabox-core-middleware",
    "version": "1.0.0",
    "description": "Ginjabox Layer API",
    "private": true,
    "scripts": {
        "start": "node ./dist/src/server",
        "build": "tsc",
        "seed": "sequelize db:seed",
        "seed:all": "sequelize db:seed:all",
        "seed:undo": "sequelize db:seed:undo",
        "seed:undo:all": "sequelize db:seed:undo:all",
        "migrate": "sequelize db:migrate",
        "migrate:all": "sequelize db:migrate:all",
        "migrate:undo": "sequelize db:migrate:undo",
        "migrate:undo:all": "sequelize db:migrate:undo:all",
        "start:dev": "ts-node-dev --respawn ./src/server.ts",
        "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold\"  \"npm:watch-ts\" \"npm:watch-node\"",
        "watch-node": "nodemon dist/src/server.js",
        "watch-ts": "tsc -w",
        "pretest": "NODE_ENV=test npm run migrate:undo:all && NODE_ENV=test npm run migrate",
        "test": "NODE_ENV=test jest --setupFiles dotenv/config"
    },
    "dependencies": {
        "@babel/cli": "^7.5.5",
        "@babel/core": "^7.5.5",
        "@babel/node": "^7.5.5",
        "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
        "@babel/preset-env": "^7.5.5",
        "@babel/preset-typescript": "^7.3.3",
        "@babel/register": "^7.5.5",
        "@sendgrid/mail": "^6.4.0",
        "@types/bluebird": "^3.5.26",
        "@types/cookie-parser": "^1.4.1",
        "@types/cors": "^2.8.5",
        "@types/crypto-js": "^3.1.43",
        "@types/debug": "^4.1.4",
        "@types/dotenv": "^6.1.1",
        "@types/express": "^4.16.1",
        "@types/jsonwebtoken": "^8.3.2",
        "@types/lodash": "^4.14.125",
        "@types/morgan": "^1.7.35",
        "@types/node-cron": "^2.0.2",
        "@types/uuid": "^3.4.4",
        "@types/validator": "^10.11.0",
        "@types/validatorjs": "^3.15.0",
        "@types/ws": "^7.2.1",
        "accesscontrol": "^2.2.1",
        "africastalking": "^0.4.3",
        "amqplib": "^0.5.5",
        "apollo-errors": "^1.9.0",
        "apollo-server-errors": "^2.2.2-alpha.1",
        "axios": "^0.19.0",
        "bcryptjs": "^2.4.3",
        "card-validator": "^6.2.0",
        "compression": "^1.7.4",
        "concurrently": "^4.1.1",
        "cookie-parser": "^1.4.4",
        "cors": "^2.8.5",
        "crypto-js": "^3.1.9-1",
        "debug": "^4.1.1",
        "dotenv": "^8.0.0",
        "ejs": "^2.6.1",
        "express": "^4.16.4",
        "global": "^4.4.0",
        "graphql-yoga": "^1.18.3",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.15",
        "moment": "^2.24.0",
        "morgan": "^1.9.1",
        "mysql2": "^1.6.5",
        "node-cron": "^2.0.3",
        "node-expat": "^2.3.18",
        "paystack-node": "^0.2.2",
        "pg": "^7.12.1",
        "pg-hstore": "^2.3.3",
        "prisma": "^1.34.10",
        "prisma-client-lib": "^1.34.10",
        "sequelize": "^5.8.5",
        "sequelize-cli": "^5.5.0",
        "servicebus": "^2.3.3",
        "uuid": "^3.3.3",
        "validatorjs": "^3.15.1",
