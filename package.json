        "eslint": "^6.1.0",
        "eslint-config-airbnb-typescript": "^4.0.1",
        "eslint-config-prettier": "^6.0.0",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-prettier": "^3.1.0",
        "faker": "^4.1.0",
        "jest": "^24.8.0",
        "nock": "^10.0.6",
        "nodemon": "^1.19.1",
        "prettier": "^1.18.2",
        "supertest": "^4.0.2",
